<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/22"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controllers.DossierPatientsControllers.BOController"
            prefHeight="500.0" prefWidth="800.0">

    <VBox alignment="CENTER" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
        <Label fx:id="patientNameLabel" text="Patient Name" style="-fx-font-size: 17; -fx-font-weight: 700; -fx-padding: 15px" alignment="CENTER" />
        <!-- Other UI components -->

        <ListView fx:id="bilanListView"/>
        <VBox fx:id="bilanDetailContainer"/>

        <Button prefHeight="25.0" prefWidth="75.0" style="-fx-padding: 10px; -fx-font-size: 15; -fx-border-radius: 15; -fx-background-color: #053c7c" text="Back" textFill="white" onAction="#handleBackToPatientDetails"/>
    </VBox>


</AnchorPane>
